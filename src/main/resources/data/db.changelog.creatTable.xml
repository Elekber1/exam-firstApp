    <changeSet id="1" author="alakbar">
        <createTable tableName="weather1">
            <column name="id" type="int8" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"></constraints>
            </column>
            <column name="city" type="varchar(255)"></column>
            <column name="date" type="date"></column>
            <column name="temp" type="varchar(255)"></column>
        </createTable>
    </changeSet>



